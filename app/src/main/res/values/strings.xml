<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <string name="app_name">Geography Puzzles</string>
    <string name="app_explanation">Hello and welcome to the app!
        \n\nYour goal is to get me from one country to another by crossing as few borders as possible. Why? That\'s quite nosy of you. Perhaps I\'m in trouble with the law and would like to avoid customs, who knows. Accomplish your task and I\'ll reward you handsomely with some of the gold coins I have sewn into my vest!</string>

    <string name="points_counter">points_counter</string>
    
    <string name="title_activity_solve_puzzle">Solve Puzzle</string>
    <string name="title_activity_review_map">Review Map</string>
    <string name="action_settings">Settings</string>
    
    <string name="arrow_description">An arrow pointing downward</string>
    <string name="creating_database">Creating database&#8230;</string>
    
    <string name="points_statement_zero">You haven\'t earned any gold coins&#8230; time to show me what you can do!</string>
    <plurals name="points_statement">
        <item quantity="one">You\'ve earned &lt;b>%d gold coin&lt;/b> &#8212; nice start!</item>
        <item quantity="other">You\'ve earned &lt;b>%d gold coins&lt;/b>.</item>
    </plurals>
    
    <string name="button_level_0">Novice</string>
    <string name="button_level_1">Traveler</string>
    <string name="button_level_2">Voyager</string>
    
    <string name="level_1_locked">Do you have what it takes? Earn %s gold coins and I\'ll reconsider.</string>
    <string name="level_2_locked">A voyager who hasn\'t earned %s gold coins? I doubt it. Now get out there and prove yourself!</string>
    
    <string name="button_give_up">Give up?</string>
    <string name="give_up_message">What?! Are you seriously quitting on me?</string>
    
    <string name="button_back_to_main">Main Menu</string>
    <string name="button_new_puzzle">New Puzzle</string>
    <string name="button_review_map">Review Map</string>
    
    <string name="puzzle_statement_format_string">I\'m in &lt;b>%1$s&lt;/b> and I\'d like to get to &lt;b>%2$s&lt;/b>.</string>
    <plurals name="path_length_statement">
        <item quantity="one">Can you help me? I\'d like to cross as few borders as possible.  I\'ve heard I can get there by travelling through only %d other country.</item>
        <item quantity="other">Can you help me? I\'d like to cross as few borders as possible.  I\'ve heard I can get there by travelling through only %d other countries.</item>
    </plurals>
    
    <string-array name="correct_guess_congratulations">
        <item>Excellent, we\'re making progress!</item>
        <item>That\'s it, now we\'re talking!</item>
        <item>Very nice work!</item>
        <item>Wow &#8212; how did you know that?</item>
        <item>Congratulations, that\'s correct!</item>
        <item>Are you some sort of geography guru?</item>
        <item>You have an excellent mental map, if I may say so myself.</item>
        <item>Nice work! With a guide like you, we\'ll get there in no time.</item>
        <item>You know what? I\'m glad I hired you.</item>
        <item>Outstanding! Marvellous work!</item>
        <item>You\'re moving so fast I can barely keep up.</item>
    </string-array>
    
    <plurals name="reward">
        <item quantity="one">You\'ve been a great guide &#8212; please accept this gold coin.</item>
        <item quantity="other">You\'ve been a great guide &#8212; please accept these %d gold coins.</item>
    </plurals>

    <string name="where_next">Where should I go next?</string>
